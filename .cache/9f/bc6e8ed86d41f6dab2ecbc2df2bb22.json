$secondary: #AB2346;\r\n$tertiary: #232ED1;\r\n\r\n//Standard colors\r\n$dark: #0D1317;\r\n$light: #FFFFFF;\r\n$card-back: #333;\r\n$shadow: rgba(0, 0, 0, 0.2);\r\n$line: #545581;\r\n\r\n//Font family and weights\r\n$family: 'Open Sans', sans-serif;\r\n$light-font: 100;\r\n$regular-font: 400;\r\n$bold-font: 700;","sass\\components\\_services.scss":"//Make service look like a card\r\n.service {\r\n  position: relative;\r\n  top: 0;\r\n  background-color: $card-back;\r\n  height: 65vh;\r\n  border-radius: 6px;\r\n  padding: 2rem;\r\n  color: $light;\r\n  box-shadow: 0 .25rem .25rem $shadow, 0 0 1rem $shadow;\r\n  overflow: hidden;\r\n  transition: top ease 0.5s;\r\n\r\n  //Moves up on hover\r\n  &:hover {\r\n    top: -10px;\r\n  }\r\n\r\n  //Makes image fit correctly\r\n  .image-container {\r\n    margin: -2rem -2rem 1rem -2rem;\r\n\r\n    img {\r\n      width: 100%;\r\n      height: 40vh;\r\n      opacity: 0;\r\n      animation: ImageFadeIn .8s 1.4s forwards;\r\n    }\r\n  }\r\n\r\n  //Creates wavy divider\r\n  svg {\r\n    position: absolute;\r\n    left: 0;\r\n    top: 40px;\r\n\r\n    .line {\r\n      opacity: 0;\r\n      animation: LineFadeIn .8s .8s forwards ease-in;\r\n    }\r\n  }\r\n\r\n  //Positions card content\r\n  .content {\r\n    margin-top: -2rem;\r\n    opacity: 0;\r\n    animation: ContentFadeIn .8s 1.6s forwards;\r\n\r\n    .title {\r\n      color: $light;\r\n      margin-top: 0;\r\n      font-weight: $bold-font;\r\n      letter-spacing: 0.01em;\r\n    }\r\n  }  \r\n}\r\n\r\n\r\n//Image divider fade in\r\n@keyframes LineFadeIn {\r\n  0% { \r\n    opacity: 0; \r\n    d: path(\"M 0 300 Q 0 300 0 300 Q 0 300 0 300 C 0 300 0 300 0 300 Q 0 300 0 300 \"); \r\n    stroke: $light; \r\n  }\r\n  50% { \r\n    opacity: 1; \r\n    d: path(\"M 0 300 Q 50 300 100 300 Q 250 300 350 300 C 350 300 500 300 650 300 Q 750 300 800 300\"); \r\n    stroke: $line; \r\n  }\r\n  100% { \r\n    opacity: 1; \r\n    d: path(\"M -2 100 Q 50 200 100 250 Q 250 400 350 300 C 400 250 550 150 650 300 Q 750 450 802 400\"); \r\n    stroke: $line; }\r\n}\r\n\r\n\r\n//Title and content fade in\r\n@keyframes ContentFadeIn {\r\n  0% { \r\n    transform: translateY(-1rem); \r\n    opacity: 0; \r\n  }\r\n  100% { \r\n    transform: translateY(0); \r\n    opacity: 1; \r\n  }\r\n}\r\n\r\n\r\n//Service imag fade in\r\n@keyframes ImageFadeIn {\r\n  0% { \r\n    transform: translate(-.5rem, -.5rem) scale(1.05); \r\n    opacity: 0; \r\n    filter: blur(2px); \r\n  }\r\n  50% { \r\n    opacity: 1; \r\n    filter: blur(2px); \r\n  }\r\n  100% { \r\n    transform: translateY(0) scale(1.0); \r\n    opacity: 1; \r\n    filter: blur(0); \r\n  }\r\n}","sass\\components\\_partners.scss":"//Make partners full width and gray on hover\r\n.partner {\r\n  width: 100%;\r\n  filter: grayscale(0%);\r\n\r\n  &:hover {\r\n    filter: grayscale(100%);\r\n  }\r\n}","sass\\components\\_portfolio.scss":"//Make project full width and highlight on hover\r\n.project {\r\n  width: 100%;\r\n  filter: brightness(0.6);\r\n  transition: 0.3s;\r\n\r\n  &:hover {\r\n    filter: brightness(1);\r\n  }\r\n\r\n  img {\r\n    background-size: 100% 100%; \r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n}","sass\\components\\_recommendations.scss":"//Make person's name and position match color scheme\r\n.name {\r\n  color: $light;\r\n} \r\n\r\n.position {\r\n  color: $light;\r\n  font-style: italic;\r\n  font-size: 0.8rem;\r\n}  \r\n\r\n//Make recommendation format as a paragraph\r\narticle {\r\n  text-indent: 3rem;\r\n  color: $light;\r\n  line-height: 1.9rem;\r\n  text-align: justify;\r\n  font-size: 1rem;\r\n}","sass\\layout\\_header.scss":".progress-container {\r\n  position: fixed;\r\n  top: 0;\r\n  z-index: 1;\r\n  width: 100%;\r\n  height: 0.3rem;\r\n  background: $dark;\r\n  left: -1px;\r\n\r\n  .progress-bar {\r\n    height: 0.3rem;\r\n    background-color: $primary;\r\n    width: 0%;\r\n  }\r\n}\r\n\r\n.intro{\r\n  position: absolute;\r\n  top: 30%;\r\n  color: $light;\r\n\r\n  h1 {\r\n    font-size: 8rem;\r\n    font-weight: $bold-font;\r\n  }\r\n\r\n  p {\r\n    font-size: 1.1rem;\r\n  }\r\n}\r\n","sass\\layout\\_footer.scss":".icon {\r\n  width: 2rem;\r\n  height: 2rem;\r\n  margin-right: 14px;\r\n  margin-bottom: 20px;\r\n\r\n  &:hover {\r\n    opacity: 0.8;\r\n    transform: scale(1.13);\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"bc317d4c2cb6554cb5650d8816851af5","cacheData":{"env":{}}}